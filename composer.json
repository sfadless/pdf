{
  "name": "sfadless/pdf",
  "license": "proprietary",
  "description": "Create pdf files from twig templates",
  "version": "0.0.1",
  "autoload": {
    "psr-4": {
      "Sfadless\\Pdf\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sfadless\\Pdf\\Test\\" : "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "mpdf/mpdf": "^v8.2.2",
    "twig/twig": "^v3.8.0",
    "symfony/event-dispatcher": "^6.4 || ^7.0",
    "symfony/options-resolver": "^6.4 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.0"
  }
}
